# Supabase project URL — found in Project Settings > API
VITE_SUPABASE_URL=https://your-project.supabase.co

# Supabase anon/public key — found in Project Settings > API
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# Your Telegram chat ID — used when inserting new reminders
# Get it by messaging @userinfobot on Telegram
VITE_TELEGRAM_CHAT_ID=your-telegram-chat-id
